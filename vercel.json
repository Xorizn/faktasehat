{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/css/(.*)",
      "dest": "/assets/css/$1"
    },
    {
      "src": "/assets/fonts/(.*)",
      "dest": "/assets/fonts/$1"
    },
    {
      "src": "/assets/img/(.*)",
      "dest": "/assets/img/$1"
    },
    {
      "src": "/assets/js/(.*)",
      "dest": "/assets/js/$1"
    },
    {
      "src": "/assets/mail/(.*)",
      "dest": "/assets/mail/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ]
}
